{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:dev": "nodemon ./server/bin/dev",
    "lint": "eslint server/src/funcs/funcs.js",
    "build": "babel server/src -s -d server/trans",
    "test": "npm run build && mocha server/trans/test/ --timeout 25000 --exit",
    "start": "npm run build && node ./server/bin/production",
    "start:react": "webpack -w -d",
    "webpack:dev": "webpack-dev-server --hot --inline",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "migrate:test": "node_modules/.bin/sequelize db:migrate --env test",
    "undo:migrate:test": "node_modules/.bin/sequelize db:migrate:undo:all --env test",
    "undo:migrate": "node_modules/.bin/sequelize db:migrate:undo:all",
    "remove-trans": "rm -rf ./server/trans",
    "integrate1": "istanbul cover ./node_modules/mocha/bin/_mocha -- -R spec ./server/trans/test/ --timeout 10000  --exit",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec ./server/trans/test/ && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage  --exit"
  },
